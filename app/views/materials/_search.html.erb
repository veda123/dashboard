<% flash.each do |key, value| %>
    <div class="alert alert-<%= key %> alert-dismissible">
        <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <%= value %>
    </div>
<% end %>

<% if @materialData && @show_pagination %>
<table class="table table-hover">
<thead class="thead-dark">
  <tr>
    <th>Serial Number</th>
    <th>ThalesID</th>
    <th>RepairHistory</th>
    <th>InstallationDate</th>
    <th>ActiveStatus</th>
  </tr>
</thead>
<tbody>
<%  @materialData.each do |data| %>
  <tr>
    <td><%= data["serialNumber"] %></td>
    <td><%= data["thalesID"] %> </td>
    <td><%= data["repairHistory"] %></td>
    <td><%= data["installationDate"] %> </td>
    <td> <%= data["activeStatus"] %></td>
  </tr>
  <% end %> 
</tbody>
</table>
<%= will_paginate @materialData, renderer: BootstrapPagination::Rails %>
<% end %>


